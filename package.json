{
  "name": "my-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://your-username.github.io/your-repo",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "backend": "node server.js",
    "backend:dev": "node --watch server.js",
    "backend:start": "node server.js",
    "backend:prod": "NODE_ENV=production node server.js",
    "db:check": "node db-setup.js check",
    "db:create-table": "node db-setup.js create-table",
    "db:list-csv": "node db-setup.js list-csv",
    "start:all": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "fix": "npm install && npm run dev",
    "test:connection": "node -e \"import('./src/services/apiService.js').then(m => m.apiService.checkConnection().then(ok => console.log('Connection:', ok ? '✅ OK' : '❌ Failed')))\"",
    "check:deployment": "node check-deployment.js",
    "test:vercel": "node test-vercel-deployment.js"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@supabase/supabase-js": "^2.49.8",
    "algoliasearch": "^4.24.0",
    "antd": "^5.25.3",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.19.2",
    "helmet": "^8.1.0",
    "i18next": "^25.2.1",
    "morgan": "^1.10.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-breadcrumbs-dynamic": "^1.2.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.5.0",
    "react-instantsearch-dom": "^6.40.4",
    "react-js-pagination": "^3.0.3",
    "react-router-dom": "^6.30.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5"
  }
}
